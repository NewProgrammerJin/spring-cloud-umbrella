<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mapper.SysUserInfoMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.entity.SysUserInfo">
        <id column="id" property="id" />
        <result column="sys_user_name" property="sysUserName" />
        <result column="sys_user_account" property="sysUserAccount" />
        <result column="sys_user_password" property="sysUserPassword" />
        <result column="sys_status" property="sysStatus" />
        <result column="sys_other_info" property="sysOtherInfo" />
        <result column="creation_time" property="creationTime" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, sys_user_name, sys_user_account, sys_user_password, sys_status, sys_other_info, creation_time
    </sql>

</mapper>
